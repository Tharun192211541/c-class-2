#include<stdio.h>
#include<string.h>
int main()
{
	char str[30];
	int count=0,i;
	printf(" enter the statement");
	for(i=0;i<30;i++)
	{
	scanf("%c",&str[i]);
    }
	for(i=0;i<30;i++)
	{
		if((str[i]=='a')||(str[i]=='e')||(str[i]=='i')||(str[i]=='o')||(str[i]=='u'))
		{
			count++;
		}
	}
	printf("%d",count);
	return 0;
}
